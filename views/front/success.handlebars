<div class="box-body">
  <form role="form" method="post">
    <!-- text input -->
    
    <div class="form-group">
      <div class="col-lg-12">
          
          <div class="col-sm-4"></div>

          <div class="col-sm-4 animated tada">  
            <img src="/images/tini_happy.png" width="400" class="img-responsive">
          </div>
          
          <div class="col-sm-4"></div>
      </div>
      <div class="col-lg-12">
        <div class="col-lg-4"></div>
        <div class="col-lg-4 text-center">

          <div class="form-group clearfix"></div>

          {{#if CorrectAnswer }}
            
            <div class="alert alert-success success-rounded text-center animated tada" role="alert">
              {{{ CorrectAnswer }}} 
            </div>

          {{/if}}

        </div>
        <div class="col-lg-4"></div>
      </div>

      <div class="form-group clearfix"></div>

      <div class="col-lg-12">
        <div class="col-lg-4"></div>
        <div class="col-lg-4 text-center">

          <a href="/guess/level/{{nextLevel}}" class="btn btn-lg button_blue"><span class="btn-submitNext">NEXT</span></a>
          <br>
          <a href="/level">Kembali ke Level</a>
          
        </div>
        <div class="col-lg-4"></div>

      </div>

    </div>
  </form>
</div>
