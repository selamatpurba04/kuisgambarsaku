<!-- box-body -->
<div class="box-body">

  <form role="form" method="post">
    <!-- text input -->
    
    <div class="form-group clearfix"></div>

    <div class="form-group">

      <div class="col-lg-12">
          
          <div class="col-sm-6">
            
            <div class="col-sm-4"></div>

            <div class="col-sm-7 alert-rounded animated tada clearfix whiteBackground">

              <!--list the top scorer -->
              <table class="table table-responsive">

                <tr>

                  <th>
                    Username
                  </th>
                  <th class="text-right">
                    Top Score
                  </th>

                </tr>

                {{#each listHighscore}}

                  <tr>

                    <td>

                      {{username}}

                    </td>

                    <td class="text-right">

                      {{current_value}}

                    </td>

                  </tr>

                {{/each}}

              </table>

            </div>

          </div>

          <div class="col-sm-6 animated tada">  

            <img src="/images/tini_happy.png" width="300" class="img-responsive margintop10px">

          </div>

      </div>

      <div class="col-lg-12">

        <div class="col-lg-4"></div>

        <div class="col-lg-4 text-center">

          <div class="form-group clearfix"></div>

          {{#if CorrectAnswer }}
            
            <div class="alert alert-success success-rounded text-center animated tada" role="alert">

              {{{ CorrectAnswer }}} 
              
            </div>

          {{/if}}

        </div>

        <div class="col-lg-4"></div>

      </div>

      <div class="form-group clearfix"></div>

      <div class="col-lg-12">

        <div class="col-lg-4"></div>

        <div class="col-lg-4 text-center">

          <a href="/level" class="btn btn-lg button_blue"><span class="btn-submitNext">LEVEL</span></a>
          <br>
          <a href="/keluar">Keluar</a>
          
        </div>

        <div class="col-lg-4"></div>

      </div>

    </div>

  </form>

</div>
<!-- /.box-body -->
